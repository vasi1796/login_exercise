generator client {
  provider = "prisma-client-js"
  binaryTargets = "debian-openssl-1.1.x"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:postgres@db:5432/postgres?connect_timeout=300"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  user_password String
  jwt_hash String?
  totp String?
  name  String?
}
